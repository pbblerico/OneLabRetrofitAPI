<?xml version="1.0" encoding="utf-8"?>
<!--suppress NavigationFile -->
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/character_nav"
    app:startDestination="@id/characterListFragment2">

    <fragment
        android:id="@+id/chaarcterInfoFragment"
        android:name="com.example.onelabretrofitapi.presentation.ChaarcterInfoFragment"
        android:label="fragment_chaarcter_info"
        tools:layout="@layout/fragment_chaarcter_info" />
    <fragment
        android:id="@+id/characterListFragment2"
        android:name="com.example.onelabretrofitapi.presentation.CharacterListFragment"
        android:label="fragment_character_list"
        tools:layout="@layout/fragment_character_list" >
        <action
            android:id="@+id/action_characterListFragment2_to_chaarcterInfoFragment"
            app:destination="@id/chaarcterInfoFragment">
            <argument android:name="characterId"
                app:argType="integer"
                android:defaultValue="0"
                />
        </action>

    </fragment>
</navigation>